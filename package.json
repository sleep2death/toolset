{
  "name": "toolset",
  "version": "1.0.1",
  "description": "a toolset for game design and dev import",
  "main": "static/index.html",
  "scripts": {
    "test": "mocha",
    "start": "node index.js"
  },
  "keywords": [
    "toolset",
    "game",
    "design",
    "excel"
  ],
  "xo": {
    "esnext": true,
    "env": [
      "node",
      "browser"
    ],
    "space": true,
    "semicolon": false,
    "rules": {
      "curly": 0,
      "keyword-spacing": 0,
      "guard-for-in": 0
    },
    "globals": [
      "$"
    ]
  },
  "author": "aspirin2d@outlook.com",
  "license": "ISC",
  "devDependencies": {
    "xo": "^0.16.0"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "chalk": "^1.1.3",
    "path": "^0.12.7",
    "svn-interface": "^0.5.0",
    "xlsx": "^0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sleep2death/toolset.git"
  }
}
